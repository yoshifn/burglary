<h1>Editing Bank #<%= @bank.id %></h1>

<form action="/banks/<%= @bank.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @bank.name  %>" type="text" />
  </div>
  <div>
    <label for="location">location</label><br />
    <input id="location" name="location" value="<%= @bank.location  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


