<h1>Editing Gang #<%= @gang.id %></h1>

<form action="/gangs/<%= @gang.id %>/update">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" value="<%= @gang.name  %>" type="text" />
  </div>
  <div>
    <label for="lord">lord</label><br />
    <input id="lord" name="lord" value="<%= @gang.lord  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


